function main() {
    // Declare and initialize variables
    let gold = "United States of America";
    let silver = "Australia";
    let bronze = "France";
    let other = "Japan";

    // Print men winners
    console.log("Men's Basketball Winners:")
    console.log("Gold: " + gold + "");
    console.log("Silver: " + bronze + "");
    console.log("Bronze: " + silver + "");
    
    console.log()

    // Print women winners
    console.log("Women's Basketball Winners:")
    console.log("Gold: " + gold + "");
    console.log("Silver " + other + "");
    console.log("Bronze: " + bronze + "");
    
    
}

main();
